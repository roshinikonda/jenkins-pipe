pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', 
                    credentialsId: 'token-for-git', 
                    url: 'https://github.com/roshinikonda/jenkins-pipe.git'
            }
        }
        stage('Build') {
            steps {
                echo "Hello from Java Pipeline"
            }
        }
    }
}
