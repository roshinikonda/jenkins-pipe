pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', 
                    credentialsId: 'your-credentials-id', 
                    url: 'https://github.com/your-user/your-repo.git'
            }
        }
        stage('Build') {
            steps {
                echo "Hello from Java Pipeline"
            }
        }
    }
}
